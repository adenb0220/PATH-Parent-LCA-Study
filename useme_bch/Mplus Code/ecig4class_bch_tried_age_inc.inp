TITLE:      E-Cig 4 Class BCH - Child Tried, Age, Income (12.19.2025)
    DATA:       FILE IS mplus_ecig.csv;
    VARIABLE:   NAMES ARE p_rule_ec p_use_ec p_know p_rule_b
                          p_use_b c_try_ec dem_gen dem_age dem_inc dem_hisp weight;
                USEV ARE p_rule_ec p_use_ec p_know p_rule_b p_use_b;
                CATEGORICAL = p_rule_ec p_use_ec p_know p_rule_b p_use_b;
                CLASSES = c(4);
                Missing are ALL (999);
		Auxiliary = c_try_ec (bch) dem_age (bch) dem_inc (bch);
                !BCH is preferred method for continuous distal outcomes
                !DCAT  preferred method for categorical outcomes
                !(Asparouhov & Muthen 2014)
    ANALYSIS:
                TYPE = MIXTURE;
                starts=1000 200;
    OUTPUT:     tech11 tech14;
    PLOT:	    type = plot3;
     		     series = p_rule_ec p_use_ec p_know p_rule_b p_use_b(*);

